# Example environment configuration for altcha
# Copy to .env and adjust values as needed

# REQUIRED: change this to a long random string, do NOT use the default
SECRET=change-me-to-a-long-random-string

# Optional settings
PORT=3000
EXPIREMINUTES=10
COMPLEXITY=1000000
MAXRECORDS=1000

STORE=memory

# Enable a demo page on http://localhost:8000 when true
DEMO=true
DEMO_PORT=8000

# GeoIP: MaxMind license key (optional, enables country-level stats)
# GEOIP_LICENSE_KEY=your-license-key-here
